insights:Median_Incomes_by_County_query sp:text """SELECT ?FIPS_County_Code ?FIPS_County_Code_Median_Household_Income_2013 ?State_label WHERE  {
  ?FIPS_County_Code semoss:Median_Household_Income_2013 ?FIPS_County_Code_Median_Household_Income_2013 .
  ?FIPS_County_Code rdf:type semoss:FIPS_County_Code  .
  ?State rdfs:label ?State_label .
  ?State rdf:type semoss:State  .
  ?State semoss:Consists ?FIPS_County_Code .
}""" ;
	a sp:Select .

